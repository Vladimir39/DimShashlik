price}>\r\n              <p>{price * amount} руб.</p>\r\n            </div>\r\n            <div className={styles.input}>\r\n              <div className={styles.quantityButton} onClick={addCountMinus}>\r\n                <p>-</p>\r\n              </div>\r\n              <div>\r\n                <p className={styles.newInput}>{amount}</p>\r\n              </div>\r\n              <div className={styles.quantityButton} onClick={addCountPlus}>\r\n                <p>+</p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div className={styles.itemClear}>\r\n        <img className={styles.clear} src={clear} onClick={deleteItem} />\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default CartItem;\r\n","export default __webpack_public_path__ + \"static/media/BasketZero.41108a47.png\";","export default __webpack_public_path__ + \"static/media/ClearTop.d7d66a49.svg\";","export default __webpack_public_path__ + \"static/media/Back.24c37844.svg\";","import React, { useState } from \"react\";\r\nimport classes from \"./Delivery.module.css\";\r\nimport back1 from \"../../../img/Back.svg\";\r\nimport { useDispatch } from \"react-redux\";\r\nimport { addDelivery } from \"../../../redux/slices/basketSlice\";\r\nimport { useForm } from \"react-hook-form\";\r\n\r\nfunction Delivery({ back, register, addDataItemTime }) {\r\n  const dispatch = useDispatch();\r\n  const { handleSubmit } = useForm();\r\n  const [timeClass, setTimeClass] = useState(classes.inputTime);\r\n  const [deliveryTime, setDeliveryTime] = useState(true);\r\n  const [activePickup, setActivePickup] = useState(classes.checkboxNotActive);\r\n  const [activeDelivery, setActiveDelivery] = useState(classes.checkboxActive);\r\n  const [selectedValue, setSelectedValue] = useState(\"Доставка\");\r\n\r\n  const onSubmit = (data) => {\r\n    // Обработка данных формы\r\n    dispatch(addDelivery(data));\r\n    console.log(data);\r\n  };\r\n  const handleRadioC